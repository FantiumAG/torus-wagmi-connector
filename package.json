{
    "name": "@fantium/torus-wagmi-connector",
    "version": "1.0.0-alpha.0",
    "description": "wagmi v2 connector to connect with Torus Wallet",
    "repository": "github:FantiumAG/torus-wagmi-connector",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "keywords": ["torus", "wagmi", "connector", "wallet"],
    "scripts": {
        "clean": "rimraf dist",
        "watch": "tsup --watch",
        "build": "tsup"
    },
    "dependencies": {
        "@biomejs/biome": "^1.9.4",
        "@toruslabs/torus-embed": "^5.0.0",
        "loglevel": "^1.8.1"
    },
    "devDependencies": {
        "@types/node": "^20.17.16",
        "lefthook": "^1.10.10",
        "rimraf": "^5.0.1",
        "tsup": "^8.3.6",
        "typescript": "^5.2.2",
        "viem": "^2.22.16",
        "wagmi": "^2.14.9"
    },
    "peerDependencies": {
        "viem": "^2.x",
        "wagmi": "^2.x"
    },
    "files": ["dist", "src"],
    "publishConfig": {
        "access": "public"
    }
}
